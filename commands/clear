module.exports = {
    name: 'clear',
    description: 'fills up the screen with many lines of periods, hiding all previous chats',
    execute(message, args) {

        return message.channel.send('Excuse me a moment.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\n.\nThere! No spoilers!');

    },
};
